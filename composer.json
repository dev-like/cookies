{
    "name": "likedsg/cookie-consent",
    "description": "Consentimento de cookies",
    "keywords": [
        "likedsg",
        "cookie-consent",
        "cookie",
        "comply",
        "law",
        "eu"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Like",
            "email": "dev@likedsg.com",
            "homepage": "https://likedsg.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/view": "^8.0|^9.0|^10.0",
        "illuminate/cookie": "^8.0|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "pestphp/pest": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Likedsg\\CookieConsent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Likedsg\\CookieConsent\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Likedsg\\CookieConsent\\CookieConsentServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
