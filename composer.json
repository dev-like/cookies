{
    "name": "likedsg/cookie-consent",
    "description": "Make your Laravel app comply with the crazy EU cookie law",
    "keywords": [
        "likedsg",
        "cookie-consent",
        "cookie",
        "comply",
        "law",
        "eu"
    ],
    "homepage": "https://github.com/spatie/cookie-consent",
    "license": "MIT",
    "authors": [
        {
            "name": "Like",
            "email": "dev@likedsg.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^8.0",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/view": "^8.0|^9.0",
        "illuminate/cookie": "^8.0|^9.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "orchestra/testbench": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Likedsg\\CookieConsent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Likedsg\\CookieConsent\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Likedsg\\CookieConsent\\CookieConsentServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
